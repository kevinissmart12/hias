{"ast":null,"code":"export default {\n  name: 'mainContent',\n  data() {\n    return {\n      discussionList: [],\n      currentPage: 1,\n      pagesize: 2\n    };\n  },\n  methods: {\n    getDiscussion() {\n      this.$axios({\n        url: '/api/discussion/getAll',\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 401) return;\n        if (res.data.status !== 200) {\n          return this.$message.error(res.data.data.msg);\n        } else {\n          this.discussionList = res.data.data.data.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n          //store\n          this.$store.commit('discussion/setDiscussionList', res.data.data.data);\n        }\n      });\n    },\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.discussionList = this.Discussion.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n    },\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      this.discussionList = this.Discussion.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n    },\n    goReply(id) {\n      //传递did和是否显示回复\n      this.$store.commit('discussion/setDid', id);\n      this.$store.commit('discussion/setShowReply', true);\n    }\n  },\n  created() {\n    this.getDiscussion();\n  },\n  computed: {\n    Discussion() {\n      return this.$store.state.discussion.discussionList;\n    },\n    DiscussionLength() {\n      return this.$store.state.discussion.discussionList.length;\n    }\n  },\n  watch: {\n    Discussion(n, o) {\n      // console.log(n,o);\n    }\n  }\n};","map":{"version":3,"mappings":"AA2CA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MAEA;QACAC;QACAC;MACA;QACAC;QACA;QACA;UACA;QACA;UACA;UACA;UACA;QACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;IAEA;IACAC;MAEA;MACA;MACA;IAEA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAF;MACA;IAAA;EAEA;AACA","names":["name","data","discussionList","currentPage","pagesize","methods","getDiscussion","url","method","console","handleSizeChange","handleCurrentChange","goReply","created","computed","Discussion","DiscussionLength","watch"],"sourceRoot":"src/components/discussion","sources":["Main.vue"],"sourcesContent":["<template>\r\n    <div class=\"mainContent\">\r\n\r\n        <el-card class=\"card\">\r\n\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <span>讨论</span>\r\n                <el-button style=\"float: right; padding: 3px 0\" type=\"text\">操作按钮</el-button>\r\n            </div>\r\n\r\n            <el-card class=\"discussion\" v-for=\"(item,index) in discussionList\" :key=\"index\" >\r\n\r\n                <div class=\"left\">\r\n                    <div class=\"title\">\r\n                        <span class=\"text\" @click=\"goReply(item.id)\">{{item.title}}</span>\r\n                    </div> \r\n                    <div class=\"content\">\r\n                        {{item.content}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"right\">\r\n                    <div class=\"time\">{{item.time}}</div>\r\n                    <div class=\"username\">发起人:{{item.user.name}}</div>\r\n                </div>\r\n            </el-card>\r\n\r\n            <!--  分页 -->\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"currentPage\"\r\n                :page-sizes=\"[1,2]\"\r\n                :page-size=\"pagesize\"\r\n                layout=\"total, sizes, prev, pager, next\"\r\n                :total=\"DiscussionLength\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'mainContent',\r\n    data(){\r\n        return{\r\n            discussionList:[],\r\n            currentPage:1,\r\n            pagesize:2,\r\n        }\r\n    },\r\n    methods:{\r\n        getDiscussion(){\r\n\r\n            this.$axios({\r\n                url:'/api/discussion/getAll',\r\n                method:\"GET\"\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==401)return\r\n                if(res.data.status!==200){\r\n                    return this.$message.error(res.data.data.msg);\r\n                }else{\r\n                    this.discussionList=res.data.data.data.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n                    //store\r\n                    this.$store.commit('discussion/setDiscussionList',res.data.data.data)\r\n                }\r\n            })\r\n        },\r\n        handleSizeChange (size) {\r\n            this.pagesize = size;\r\n            this.discussionList=this.Discussion.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n        },\r\n        handleCurrentChange (currentPage) {\r\n            this.currentPage = currentPage;\r\n            this.discussionList=this.Discussion.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n\r\n        },\r\n        goReply(id){\r\n            \r\n            //传递did和是否显示回复\r\n            this.$store.commit('discussion/setDid',id)\r\n            this.$store.commit('discussion/setShowReply',true)\r\n\r\n        }\r\n    },\r\n    created(){\r\n        this.getDiscussion()\r\n    },\r\n    computed:{\r\n        Discussion(){\r\n            return this.$store.state.discussion.discussionList\r\n        },\r\n        DiscussionLength(){\r\n            return this.$store.state.discussion.discussionList.length\r\n        }\r\n    },\r\n    watch:{\r\n        Discussion(n,o){\r\n            // console.log(n,o);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .mainContent{\r\n        width: 100%;\r\n        height: 100%;\r\n\r\n        .card{\r\n            width: 100%;\r\n            height: 100%;\r\n            overflow-y: hidden;\r\n        }\r\n    }\r\n\r\n    .card > ::v-deep .el-card__body{\r\n        padding:0;\r\n    }\r\n\r\n    .discussion{\r\n        display: flex;\r\n        flex-d\r\n        width: 100%;\r\n        background-color: #f5f5f5;\r\n\r\n        .title{\r\n            font-size:24px;\r\n            font-weight: 900;\r\n        }\r\n        .content{\r\n            font-size:16px;\r\n            min-width: 400px;\r\n            max-width: 800px;\r\n            padding:10px;\r\n        }\r\n        .text:hover{\r\n            color:rgb(64, 119, 222);\r\n            text-decoration:underline;\r\n        }\r\n        .time{\r\n\r\n            font-weight: 200;\r\n            font-size: 10px;\r\n            color:rgba(0,0,0,.8)\r\n        }\r\n        .username{\r\n\r\n        }\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}