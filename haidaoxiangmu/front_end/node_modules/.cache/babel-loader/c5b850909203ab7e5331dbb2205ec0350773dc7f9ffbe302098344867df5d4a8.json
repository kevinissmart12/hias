{"ast":null,"code":"import random from '../../api/random';\nexport default {\n  name: 'allowner',\n  data() {\n    return {\n      currentPage: 1,\n      pagesize: 5,\n      tempTableData: [],\n      // 手风琴激活\n      activeName: '1',\n      tableItem: [{\n        'id': '1',\n        prop: 'name',\n        label: '名称/法人',\n        width: '100'\n      }, {\n        'id': '2',\n        prop: 'discriminator',\n        label: '类型',\n        width: '100'\n      }, {\n        'id': '3',\n        prop: 'town',\n        label: '所在镇',\n        width: '150'\n      }, {\n        'id': '4',\n        prop: 'village',\n        label: '所在村',\n        width: '150'\n      }, {\n        'id': '5',\n        prop: 'creditLevel',\n        label: '信用等级',\n        width: '100'\n      }, {\n        'id': '6',\n        prop: 'departmentId',\n        label: '组织机构代码',\n        width: '150'\n      }, {\n        'id': '8',\n        prop: 'checkStatus',\n        label: '审核状态',\n        width: '100'\n      }],\n      form: {\n        discriminator: '',\n        id: '',\n        name: '',\n        mobile: '',\n        birthday: '',\n        sex: '',\n        imgUrl: '',\n        address: '',\n        town: '',\n        village: '',\n        engagedTime: '',\n        educationLevel: '',\n        farmerTitle: '',\n        departmentId: '',\n        companyName: '',\n        establishedTime: '',\n        fixedAssets: '',\n        businessContents: '',\n        isBaseOfEmployee: '',\n        numberOfEmployee: '',\n        featuredProducts: '',\n        creditLevel: '',\n        comment: ''\n        // checkStatus:'',\n      },\n\n      formShow: false,\n      imageName: ''\n    };\n  },\n  methods: {\n    getAllOwner() {\n      this.$axios({\n        url: '/api/owner/getAll',\n        method: 'GET'\n      }).then(res => {\n        if (res.data.status == 401) {\n          return;\n        }\n        if (res.data.status != 200) {\n          return this.$message.error(res.data.data.msg);\n        }\n        console.log(res.data);\n        this.tempTableData = res.data.data.data.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n        this.tempTableData.forEach((i, v) => {\n          if (i.checkStatus == 0) {\n            i.checkStatus = '审核中';\n          } else {\n            i.checkStatus = '已通过';\n          }\n        });\n        this.$store.commit('allOwner/setTableData', res.data.data.data);\n      });\n    },\n    check(item) {\n      // console.log(item);\n      this.formShow = true;\n      this.form = item;\n      this.activeName = '1';\n    },\n    cancle() {\n      this.formShow = false;\n    },\n    save() {\n      // update\n      let data = this.qs.stringify(this.form);\n      this.$axios({\n        url: '/api/owner/update',\n        method: \"POST\",\n        data: data\n      }).then(res => {\n        console.log(res);\n        if (res.data.status == 401) return;\n        if (res.data.status !== 200) {\n          return this.$message.error(res.data.data.msg);\n        } else {\n          this.formShow = false;\n          this.$message.success('修改成功');\n        }\n      });\n    },\n    beforeUpload(file) {\n      this.upload(file);\n      return false;\n    },\n    //上传图片，用form-data\n    upload(file) {\n      console.log(file);\n      let type = file.name.replace(/.+\\./, \".\");\n      this.imageName = random.randomImageName(6) + type;\n      const formData = new FormData();\n      formData.append('file', file);\n      formData.append('name', this.imageName);\n      this.$axios({\n        url: '/api/owner/upload',\n        method: 'POST',\n        headers: {\n          \"Content-Type\": \"multipart/form-data\"\n        },\n        data: formData\n      }).then(res => {\n        console.log(res.data.data.imgUrl);\n        this.form.imgUrl = 'http://localhost:3000' + res.data.data.imgUrl;\n      });\n    }\n  },\n  computed: {\n    AllOwner() {\n      return this.$store.state.allOwner.tableData;\n    },\n    AllOwnerLength() {\n      return this.$store.state.allOwner.tableData.length;\n    }\n  },\n  created() {\n    this.getAllOwner();\n  }\n};","map":{"version":3,"mappings":"AAqDA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACA;MACAC;MACAC,YACA;QAAA;QAAAC;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,EACA;MACAC;QACAC;QACAC;QAEAZ;QACAa;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QAEAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACA;MACA;;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;QACAC;QACAC;MACA;QACA;UACA;QACA;QACA;UACA;QACA;QAEAC;QACA;QACA;UACA;YACAC;UACA;YACAA;UACA;QACA;QACA;MAEA;IACA;IACAC;MACA;MACA;MACA;MACA;IACA;IACAC;MACA;IAEA;IACAC;MACA;MACA;MAEA;QACAN;QACAC;QACArC;MACA;QACAsC;QACA;QACA;UACA;QACA;UACA;UACA;QACA;MACA;IACA;IACAK;MACA;MAEA;IACA;IACA;IACAC;MACAN;MACA;MACA;MAEA;MACAO;MACAA;MAEA;QACAT;QACAC;QACAS;UAAA;QAAA;QACA9C;MACA;QACAsC;QACA;MACA;IACA;EAEA;EACAS;IACAC;MACA;IACA;IACAC;MACA;IACA;EAEA;EACAC;IACA;EACA;AACA","names":["name","data","currentPage","pagesize","tempTableData","activeName","tableItem","prop","label","width","form","discriminator","id","mobile","birthday","sex","imgUrl","address","town","village","engagedTime","educationLevel","farmerTitle","departmentId","companyName","establishedTime","fixedAssets","businessContents","isBaseOfEmployee","numberOfEmployee","featuredProducts","creditLevel","comment","formShow","imageName","methods","getAllOwner","url","method","console","i","check","cancle","save","beforeUpload","upload","formData","headers","computed","AllOwner","AllOwnerLength","created"],"sourceRoot":"src/components/owner","sources":["AllOwner.vue"],"sourcesContent":["<template>\r\n    <div class=\"allowner\">\r\n\r\n        <el-container>\r\n            <el-aside width=\"200px\">\r\n\r\n            </el-aside>\r\n            <el-main>\r\n\r\n                <el-table\r\n                    :data=\"tempTableData\"\r\n                    style=\"width: 100%\">\r\n                    <el-table-column\r\n                        :prop=\"item.prop\"\r\n                        :label=\"item.label\"\r\n                        :width=\"item.width\"\r\n                        v-for=\"(item,index) in tableItem\" \r\n                        :key=\"index\"\r\n                    >\r\n                        <template slot-scope=\"scope\">\r\n                            <img v-if=\"item.prop=='imgUrl'\" :src=\"scope.row.imgUrl\" alt=\"\" width=\"90\">\r\n                            <div v-else>{{scope.row[item.prop]}}</div>\r\n                        </template>\r\n                    </el-table-column>\r\n\r\n                    <el-table-column\r\n                        fixed=\"right\"\r\n                        label=\"操作\"\r\n                        width=\"120\">\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button @click=\"check(scope.row)\" type=\"text\" size=\"small\">查看</el-button>\r\n                            <el-button  style=\"color:red\" type=\"text\" size=\"small\">删除</el-button>\r\n\r\n                        </template>\r\n                    </el-table-column>\r\n\r\n                </el-table>\r\n\r\n            </el-main>\r\n        </el-container>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport random from '../../api/random'\r\nexport default {\r\n    name:'allowner',\r\n    data(){\r\n        return{\r\n            currentPage:1,\r\n            pagesize:5,\r\n            tempTableData:[],\r\n            // 手风琴激活\r\n            activeName:'1',\r\n            tableItem:[\r\n                {'id':'1',prop:'name',label:'名称/法人',width:'100'},\r\n                {'id':'2',prop:'discriminator',label:'类型',width:'100'},\r\n                {'id':'3',prop:'town',label:'所在镇',width:'150'}, \r\n                {'id':'4',prop:'village',label:'所在村',width:'150'},\r\n                {'id':'5',prop:'creditLevel',label:'信用等级',width:'100'},\r\n                {'id':'6',prop:'departmentId',label:'组织机构代码',width:'150'},\r\n                {'id':'8',prop:'checkStatus',label:'审核状态',width:'100'},\r\n            ],\r\n            form: {\r\n                discriminator:'',\r\n                id:'',\r\n\r\n                name: '',\r\n                mobile:'',\r\n                birthday:'',\r\n                sex:'',\r\n                imgUrl:'',\r\n                address:'',\r\n                town:'',\r\n                village:'',\r\n                engagedTime:'',\r\n                educationLevel:'',\r\n                farmerTitle:'',\r\n                departmentId:'',\r\n\r\n                companyName:'',\r\n                establishedTime:'',\r\n                fixedAssets:'',\r\n                businessContents:'',\r\n                isBaseOfEmployee:'',\r\n                numberOfEmployee:'',\r\n                featuredProducts:'',\r\n                creditLevel:'',\r\n                comment:'',\r\n                // checkStatus:'',\r\n            },\r\n            formShow:false,\r\n            imageName:'',\r\n        }\r\n    },\r\n    methods:{\r\n        getAllOwner(){\r\n            this.$axios({\r\n                url:'/api/owner/getAll',\r\n                method:'GET'\r\n            }).then(res=>{\r\n                if(res.data.status==401){\r\n                    return\r\n                }\r\n                if(res.data.status!=200){\r\n                    return this.$message.error(res.data.data.msg);\r\n                }\r\n\r\n                console.log(res.data);\r\n                this.tempTableData=res.data.data.data.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n                this.tempTableData.forEach((i,v)=>{\r\n                    if(i.checkStatus==0){\r\n                        i.checkStatus='审核中'\r\n                    }else{\r\n                        i.checkStatus='已通过'\r\n                    }\r\n                })\r\n                this.$store.commit('allOwner/setTableData',res.data.data.data)\r\n\r\n            })\r\n        },\r\n        check(item){\r\n            // console.log(item);\r\n            this.formShow=true\r\n            this.form=item\r\n            this.activeName='1'\r\n        },\r\n        cancle(){\r\n            this.formShow=false\r\n            \r\n        },\r\n        save(){\r\n            // update\r\n            let data=this.qs.stringify(this.form)\r\n\r\n            this.$axios({\r\n                url:'/api/owner/update',\r\n                method:\"POST\",\r\n                data:data\r\n            }).then(res=>{\r\n                console.log(res);\r\n                if(res.data.status==401)return\r\n                if(res.data.status!==200){\r\n                    return this.$message.error(res.data.data.msg);\r\n                }else{\r\n                    this.formShow=false\r\n                    this.$message.success('修改成功');\r\n                }\r\n            })\r\n        },\r\n        beforeUpload(file) {\r\n            this.upload(file)\r\n\r\n            return false\r\n        },\r\n        //上传图片，用form-data\r\n        upload(file){\r\n            console.log(file);\r\n            let type=file.name.replace(/.+\\./, \".\")\r\n            this.imageName=random.randomImageName(6)+type\r\n\r\n            const formData=new FormData()\r\n            formData.append('file',file)\r\n            formData.append('name',this.imageName)\r\n\r\n            this.$axios({\r\n                url:'/api/owner/upload',\r\n                method:'POST',\r\n                headers:{\"Content-Type\":\"multipart/form-data\"},\r\n                data:formData\r\n            }).then(res=>{\r\n                console.log(res.data.data.imgUrl);\r\n                this.form.imgUrl='http://localhost:3000'+res.data.data.imgUrl\r\n            })\r\n        },\r\n\r\n    },\r\n    computed:{\r\n        AllOwner(){\r\n            return this.$store.state.allOwner.tableData\r\n        },\r\n        AllOwnerLength(){\r\n            return this.$store.state.allOwner.tableData.length\r\n        },\r\n\r\n    },\r\n    created(){\r\n        this.getAllOwner()\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .allowner{\r\n        position: relative;\r\n    }\r\n    \r\n    .show{\r\n        display: none;\r\n    }\r\n\r\n    .bg{\r\n        \r\n        position:fixed;\r\n        left:0;\r\n        top:0;\r\n        width: 100vw;\r\n        height: 100vh;\r\n        background-color: rgba(0,0,0,.3);\r\n        z-index:10;\r\n\r\n    }\r\n\r\n    .el-card{\r\n        \r\n        position:absolute;\r\n        top:50px;\r\n        left:50%;\r\n        transform: translateX(-50%);\r\n        width:700px;\r\n        // height: 500px;\r\n    }\r\n\r\n    .short{\r\n        width: 200px;\r\n    }\r\n\r\n    ::v-deep .el-textarea__inner{\r\n        height: 66px!important;\r\n    }\r\n    .el-form-item{\r\n        margin-bottom:5px!important;\r\n    }\r\n\r\n    .el-upload {\r\n        border: 1px dashed #d9d9d9;\r\n        border-radius: 6px;\r\n        cursor: pointer;\r\n        position: relative;\r\n        overflow: hidden;\r\n    }\r\n    .el-upload:hover {\r\n        border-color: #409EFF;\r\n    }\r\n    .avatar-uploader-icon {\r\n        font-size: 28px;\r\n        color: #8c939d;\r\n        width: 78px;\r\n        height: 78px;\r\n        line-height: 78px;\r\n        text-align: center;\r\n    }\r\n    .avatar {\r\n        width: 78px;\r\n        height: 78px;\r\n        display: block;\r\n    }\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}