      //清空\r\n                this.form.town=''\r\n                this.form.village=''\r\n            }\r\n            //重新获取town数据\r\n            this.getTown()\r\n        },\r\n        'form.town'(n,o){\r\n            if(n=='')return\r\n            //重新选择\r\n            if(n!==o){\r\n                //清空\r\n                this.form.village=''\r\n            }\r\n            this.getVillage()\r\n        },\r\n        'form.village'(n,o){\r\n            if(n=='')return\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}