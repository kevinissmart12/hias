\n        send(){\r\n            let inputValue=this.inputValue\r\n            ws.send(inputValue)\r\n        },\r\n        closeWebSocket(){\r\n            // 关闭websocket\r\n            ws.onclose = function()\r\n            { \r\n                // 关闭 websocket\r\n                console.log(\"连接已关闭...\"); \r\n            };\r\n        },\r\n        onMessage(){\r\n            \t// 接收消息后客户端处理方法\r\n            ws.onmessage = function (evt) \r\n            { \r\n            console.log(evt.data);\r\n            $('#res').text(evt.data);\r\n            };\r\n            \r\n        }\r\n    },\r\n    created() {\r\n\r\n    },\r\n    components:{\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped> \r\n    .index{\r\n        width: 100%;\r\n        height: 100%;\r\n\r\n        background-color: #fff;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}