{"ast":null,"code":"export default {\n  name: 'search',\n  props: {\n    list: []\n  },\n  data() {\n    return {\n      overLength: false\n    };\n  },\n  methods: {\n    select(item) {\n      // console.log(item);\n      this.$emit('getSelectedItem', item);\n      this.$store.commit('search/setShowList', false);\n    }\n  },\n  computed: {\n    ShowList() {\n      return this.$store.state.search.showList;\n    }\n  },\n  watch: {\n    list() {\n      this.$store.commit('search/setShowList', true);\n      if (list.length > 5) {\n        this.overLength = true;\n      } else {\n        this.overLength = false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACA;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACAP;MACA;MACA;QACA;MACA;QACA;MACA;IACA;EACA;AACA","names":["name","props","list","data","overLength","methods","select","computed","ShowList","watch"],"sourceRoot":"src/components/input-search","sources":["Search.vue"],"sourcesContent":["<template>\r\n    <div class=\"search-box\">\r\n        <div class=\"search\" :class=\"overLength?'overLength':''\">\r\n        <div class=\"search_list\"  v-for=\"(item,index) in list\" :key=\"index\" @click=\"select(item)\">\r\n            {{item.name}}\r\n        </div>\r\n    </div>\r\n    </div>\r\n    \r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'search',\r\n    props:{\r\n        list:[]\r\n    },\r\n    data(){\r\n        return{\r\n            overLength:false,\r\n        }\r\n    },\r\n    methods: {\r\n        select(item){\r\n            // console.log(item);\r\n            this.$emit('getSelectedItem',item)\r\n            this.$store.commit('search/setShowList',false)\r\n        }\r\n    },\r\n    computed:{\r\n        ShowList(){\r\n            return this.$store.state.search.showList\r\n        }\r\n    },\r\n    watch:{\r\n        list(){\r\n            this.$store.commit('search/setShowList',true)\r\n            if(list.length>5){\r\n                this.overLength=true\r\n            }else{\r\n                this.overLength=false\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .search{\r\n        position: absolute;\r\n        width: 100%;\r\n        background-color: #fff;\r\n        border:1px solid rgba(0,0,0,.2);\r\n        z-index:10;\r\n        .search_list{\r\n            padding-left:20px;\r\n            border-bottom: 1px solid rgba(0,0,0,.1);\r\n        }\r\n    }\r\n\r\n    .listShow{\r\n        display: none;\r\n    }\r\n\r\n    .overLength{\r\n        height: 205px;\r\n        overflow-y: scroll;\r\n    }\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}