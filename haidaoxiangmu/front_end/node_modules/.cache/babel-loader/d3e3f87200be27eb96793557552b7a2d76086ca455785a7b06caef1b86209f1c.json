"allowner\">\r\n\r\n        <el-table\r\n            :data=\"tempTableData\"\r\n            style=\"width: 100%\">\r\n            <el-table-column\r\n                :prop=\"item.prop\"\r\n                :label=\"item.label\"\r\n                :width=\"item.width\"\r\n                v-for=\"(item,index) in tableItem\" \r\n                :key=\"index\"\r\n            >\r\n                <template slot-scope=\"scope\">\r\n                    <img v-if=\"item.prop=='imgUrl'\" :src=\"scope.row.imgUrl\" alt=\"\" width=\"90\">\r\n                    <div v-else>{{scope.row[item.prop]==0?'审核中':'已通过'}}</div>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column\r\n                fixed=\"right\"\r\n                label=\"操作\"\r\n                width=\"120\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button @click=\"check(scope.row)\" type=\"text\" size=\"small\">查看</el-button>\r\n                    <el-button  style=\"color:red\" type=\"text\" size=\"small\">删除</el-button>\r\n\r\n                </template>\r\n            </el-table-column>\r\n\r\n        </el-table>\r\n\r\n        <transition name=\"el-fade-in-linear\">\r\n            <div class=\"bg\" v-show=\"formShow\">\r\n\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>{{form.discriminator=='个人'?\"个人养殖户\":\"企业养殖\"}}</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" @click=\"cancle\" type=\"text\">关闭</el-button>\r\n                    </div>\r\n                    <!-- form -->\r\n                    <el-form label-position=\"left\" :inline=\"true\" ref=\"form\" :model=\"form\" label-width=\"100px\">\r\n                        \r\n                        <el-collapse v-model=\"activeName\" accordion>\r\n                            <el-collapse-item title=\"个人信息\" name=\"1\">\r\n                                <el-form-item label=\"名称/法人\" >\r\n                                    <el-input class=\"short\" v-model=\"form.name\"></el-input>\r\n                                </el-form-item>\r\n\r\n                                <el-form-item :label=\"form.discriminator=='个人'?'手机号码':'联系电话'\">\r\n                                    <el-input class=\"short\" v-model=\"form.mobile\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"家庭住址\">\r\n                                    <el-input class=\"short\" v-model=\"form.address\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"所在镇\">\r\n                                    <el-input class=\"short\" v-model=\"form.town\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"所在村\">\r\n                                    <el-input class=\"short\" v-model=\"form.village\"></el-input>\r\n                                </el-form-item>\r\n\r\n                                <el-form-item label=\"出生年月\">\r\n                                    <el-date-picker\r\n                                        class=\"short\"\r\n                                        v-model=\"form.birthday\"\r\n                                        type=\"date\"\r\n                                        placeholder=\"选择日期\">\r\n                                    </el-date-picker>\r\n                                </el-form-item>\r\n\r\n                                <el-form-item label=\"性别\">\r\n                                    <el-radio-group class=\"short\" v-model=\"form.sex\">\r\n                                        <el-radio label=\"男\"></el-radio>\r\n                                        <el-radio label=\"女\"></el-radio>\r\n                                    </el-radio-group>\r\n                                </el-form-item>\r\n\r\n                                <el-form-item label=\"文化程度\">\r\n                                    <el-select v-model=\"form.educationLevel\" placeholder=\"请选择\" class=\"short\">\r\n                                    <el-option label=\"小学\" value=\"小学\"></el-option>\r\n                                    <el-option label=\"初中\" value=\"初中\"></el-option>\r\n                                    <el-option label=\"高中\" value=\"高中\"></el-option>\r\n                                    <el-option label=\"大学\" value=\"大学\"></el-option>\r\n                                    <el-option label=\"研究生\" value=\"研究生\"></el-option>\r\n                                    <el-option label=\"博士\" value=\"博士\"></el-option>\r\n\r\n                                    </el-select>\r\n                                </el-form-item>\r\n                                \r\n                                <el-form-item label=\"组织机构代码\">\r\n                                    <el-input class=\"short\" v-model=\"form.departmentId\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item label=\"信用等级\">\r\n                                    <el-select v-model=\"form.creditLevel\" placeholder=\"请选择\" class=\"short\">\r\n                                    <el-option label=\"A\" value=\"A\"></el-option>\r\n                                    <el-option label=\"B\" value=\"B\"></el-option>\r\n                                    <el-option label=\"C\" value=\"C\"></el-option>\r\n                                    </el-select>\r\n                                </el-form-item>\r\n\r\n                                <el-form-item style=\"display:block;\"  label=\"备注\">\r\n                                    <el-input style=\"width:510px\" autosize type=\"textarea\" v-model=\"form.comment\"></el-input>\r\n                                </el-form-item>\r\n\r\n                            </el-collapse-item>\r\n\r\n                            <el-collapse-item v-if=\"form.discriminator=='企业'\" title=\"企业信息\" name=\"2\">\r\n                                <!-- 和企业有关的 -->\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"公司\">\r\n                                    <el-input class=\"short\" v-model=\"form.companyName\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"成立时间\">\r\n                                    <el-date-picker\r\n                                        class=\"short\"\r\n                                        v-model=\"form.establishedTime\"\r\n                                        type=\"date\"\r\n                                        placeholder=\"选择日期\">\r\n                                    </el-date-picker>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"固定资产\">\r\n                                    <el-input class=\"short\" v-model=\"form.fixedAssets\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"经营内容\">\r\n                                    <el-input class=\"short\" v-model=\"form.businessContents\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"无公害基地\">\r\n                                    <el-input class=\"short\" v-model=\"form.isBaseOfEmployee\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"员工人数\">\r\n                                    <el-input class=\"short\" v-model=\"form.numberOfEmployee\"></el-input>\r\n                                </el-form-item>\r\n                                <el-form-item v-if=\"form.discriminator=='企业'\" label=\"特色产品\">\r\n                                    <el-input class=\"short\" v-model=\"form.featuredProducts\"></el-input>\r\n                                </el-form-item>\r\n\r\n                            </el-collapse-item>\r\n                        </el-collapse>\r\n\r\n                        <el-form-item style=\"display:block;text-align:center\">\r\n                            <el-button type=\"primary\" @click=\"save\">保存</el-button>\r\n                            <el-button @click=\"cancle\">取消</el-button>\r\n                        </el-form-item>\r\n\r\n                    </el-form>\r\n                </el-card>\r\n\r\n            </div>\r\n        </transition>\r\n\r\n\r\n\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'allowner',\r\n    data(){\r\n        return{\r\n            currentPage:1,\r\n            pagesize:5,\r\n            tempTableData:[],\r\n            // 手风琴激活\r\n            activeName:'1',\r\n            tableItem:[\r\n                {'id':'1',prop:'name',label:'名称/法人',width:'100'},\r\n                {'id':'2',prop:'discriminator',label:'类型',width:'100'},\r\n                {'id':'3',prop:'town',label:'所在镇',width:'150'}, \r\n                {'id':'4',prop:'village',label:'所在村',width:'150'},\r\n                {'id':'5',prop:'creditLevel',label:'信用等级',width:'100'},\r\n                {'id':'6',prop:'departmentId',label:'组织机构代码',width:'150'},\r\n                {'id':'8',prop:'checkStatus',label:'审核状态',width:'100'},\r\n            ],\r\n            form: {\r\n                discriminator:'',\r\n                id:'',\r\n\r\n                name: '',\r\n                mobile:'',\r\n                birthday:'',\r\n                sex:'',\r\n                imgUrl:'',\r\n                address:'',\r\n                town:'',\r\n                village:'',\r\n                engagedTime:'',\r\n                educationLevel:'',\r\n                farmerTitle:'',\r\n                departmentId:'',\r\n                companyName:'',\r\n                establishedTime:'',\r\n                fixedAssets:'',\r\n                businessContents:'',\r\n                isBaseOfEmployee:'',\r\n                numberOfEmployee:'',\r\n                featuredProducts:'',\r\n                creditLevel:'',\r\n                comment:'',\r\n            },\r\n            formShow:false,\r\n        }\r\n    },\r\n    methods:{\r\n        getAllOwner(){\r\n            this.$axios({\r\n                url:'/api/owner/getAll',\r\n                method:'GET'\r\n            }).then(res=>{\r\n                if(res.data.status==401){\r\n                    return\r\n                }\r\n                if(res.data.status!=200){\r\n                    return this.$message.error(res.data.data.msg);\r\n                }\r\n\r\n                console.log(res.data);\r\n                this.tempTableData=res.data.data.data.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n                this.$store.commit('allOwner/setTableData',res.data.data.data)\r\n\r\n            })\r\n        },\r\n        check(item){\r\n            // console.log(item);\r\n            this.formShow=true\r\n            this.form=item\r\n            this.activeName='1'\r\n        },\r\n        cancle(){\r\n            this.formShow=false\r\n            \r\n        },\r\n        save(){\r\n            // update\r\n            \r\n        },\r\n\r\n    },\r\n    computed:{\r\n        AllOwner(){\r\n            return this.$store.state.allOwner.tableData\r\n        },\r\n        AllOwnerLength(){\r\n            return this.$store.state.allOwner.tableData.length\r\n        },\r\n\r\n    },\r\n    created(){\r\n        this.getAllOwner()\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .allowner{\r\n        position: relative;\r\n    }\r\n    \r\n    .show{\r\n        display: none;\r\n    }\r\n\r\n    .bg{\r\n        \r\n        position:fixed;\r\n        left:0;\r\n        top:0;\r\n        width: 100vw;\r\n        height: 100vh;\r\n        background-color: rgba(0,0,0,.3);\r\n        z-index:10;\r\n\r\n    }\r\n\r\n    .el-card{\r\n        \r\n        position:absolute;\r\n        top:50px;\r\n        left:50%;\r\n        transform: translateX(-50%);\r\n        width:700px;\r\n        // height: 500px;\r\n    }\r\n\r\n    .short{\r\n        width: 200px;\r\n    }\r\n\r\n    ::v-deep .el-textarea__inner{\r\n        height: 66px!important;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}