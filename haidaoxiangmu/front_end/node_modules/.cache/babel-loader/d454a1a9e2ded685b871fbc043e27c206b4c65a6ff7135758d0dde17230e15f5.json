           // console.log(BMap, map)\r\n            this.center.lng = 122.048\r\n            this.center.lat = 29.9424\r\n            this.zoom = 16\r\n\r\n            this.position.lng = 122.048\r\n            this.position.lat = 29.9424\r\n\r\n            this.draggable=true\r\n\r\n        },\r\n    },\r\n    computed:{\r\n        Polygons(){\r\n            return this.$store.state.ponds.polygons\r\n        },\r\n        AddPond(){\r\n            return this.$store.state.ponds.addPond\r\n        },\r\n        EditPond(){\r\n            return this.$store.state.ponds.editPond\r\n        },\r\n    },\r\n    created(){\r\n        this.getPonds()\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}