{"ast":null,"code":"export default {\n  name: 'side',\n  data() {\n    return {\n      form: {\n        county: '',\n        town: '',\n        village: ''\n      },\n      //1=>定海区，2=>普陀区,3=>岱山县,4=>嵊泗县\n      countyOption: [{\n        value: 1,\n        label: '定海区'\n      }, {\n        value: 2,\n        label: '普陀区'\n      }, {\n        value: 3,\n        label: '岱山县'\n      }, {\n        value: 4,\n        label: '嵊泗县'\n      }],\n      townOption: [],\n      villageOption: [],\n      json_fields: {\n        所在区: \"countyCotent\",\n        所在镇: \"town\",\n        所在村: \"village\",\n        海水养殖面积: \"seaArea\",\n        海水养殖估算面积: \"seaEstimateArea\",\n        淡水养殖面积: \"freshWaterArea\",\n        淡水养殖估算面积: \"freshWaterEstimateArea\",\n        养殖总面积: \"totalArea\",\n        养殖估算总面积: \"totalEstimateArea\"\n      },\n      json_data: []\n    };\n  },\n  methods: {\n    //获取镇和村\n    getTown() {\n      let data = this.qs.stringify({\n        countyId: this.form.county\n      });\n      this.$axios({\n        url: '/api/area/getTown',\n        method: 'POST',\n        data: data\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 200) {\n          this.townOption = res.data.data.data;\n          //赋值\n        }\n      });\n    },\n\n    getVillage() {\n      let data = this.qs.stringify({\n        Tid: this.form.town\n      });\n      this.$axios({\n        url: '/api/area/getVillage',\n        method: 'POST',\n        data: data\n      }).then(res => {\n        if (res.data.status == 200) {\n          this.villageOption = res.data.data.data;\n        }\n      });\n    },\n    search() {\n      let data = this.qs.stringify(this.form);\n      this.$axios({\n        url: '/api/ponds/search',\n        method: 'POST',\n        data: data\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 200) {\n          this.$store.commit('statistic/setPondsType', res.data.data.data);\n        }\n      });\n    },\n    exportExcel() {\n      this.json_data = this.PondsType;\n    }\n  },\n  computed: {\n    PondsType() {\n      return this.$store.state.statistic.pondsType;\n    }\n  },\n  watch: {\n    'form.county'(n, o) {\n      if (n == '') return;\n      //重新选择\n      if (n !== o) {\n        //清空\n        this.form.town = '';\n        this.form.village = '';\n      }\n      //重新获取town数据\n      this.getTown();\n    },\n    'form.town'(n, o) {\n      if (n == '') return;\n      //重新选择\n      if (n !== o) {\n        //清空\n        this.form.village = '';\n      }\n      this.getVillage();\n    },\n    'form.village'(n, o) {\n      if (n == '') return;\n    }\n  }\n};","map":{"version":3,"mappings":"AAyDA;EACAA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;MACA;MACA;MACAC,eACA;QACAC;QACAC;MACA,GACA;QACAD;QACAC;MACA,GACA;QACAD;QACAC;MACA,GACA;QACAD;QACAC;MACA,EACA;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;IAGA;EACA;EACAC;IACA;IACAC;MACA;QACAC;MACA;MAEA;QACAC;QACAC;QACAzB;MACA;QACA0B;QACA;UACA;UACA;QACA;MACA;IACA;;IACAC;MAEA;QACAC;MACA;MAEA;QACAJ;QACAC;QACAzB;MACA;QACA;UACA;QACA;MACA;IACA;IACA6B;MACA;MACA;QACAL;QACAC;QACAzB;MACA;QACA0B;QACA;UACA;QACA;MACA;IACA;IACAI;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACA;MACA;MACA;MACA;QACA;QACA;QACA;MACA;MACA;MACA;IACA;IACA;MACA;MACA;MACA;QACA;QACA;MACA;MACA;IACA;IACA;MACA;IACA;EACA;AACA","names":["name","data","form","county","town","village","countyOption","value","label","townOption","villageOption","json_fields","所在区","所在镇","所在村","海水养殖面积","海水养殖估算面积","淡水养殖面积","淡水养殖估算面积","养殖总面积","养殖估算总面积","json_data","methods","getTown","countyId","url","method","console","getVillage","Tid","search","exportExcel","computed","PondsType","watch"],"sourceRoot":"src/components/statistic/PondsType","sources":["Side.vue"],"sourcesContent":["<template>\r\n    <div class=\"side\">\r\n        <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <span>养殖池塘类型统计</span>\r\n                <download-excel\r\n                    class=\"export-excel-wrapper\"\r\n                    :data=\"json_data\"\r\n                    :fields=\"json_fields\"\r\n                    type=\"xls\"\r\n                    worksheet=\"My Worksheet\"\r\n                    name=\"用户信息采样数据\"\r\n                    style=\"float: right; padding: 3px 0\"\r\n                >\r\n                    <el-button @click=\"exportExcel\" style=\"float: right; padding: 3px 0\" type=\"text\">导出统计列表</el-button>\r\n                </download-excel>\r\n            </div>\r\n            <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\r\n                    <el-form-item label=\"所在区\">\r\n                        <el-select v-model=\"form.county\" placeholder=\"请选择所在区/县\">\r\n                            <el-option\r\n                                v-for=\"item in countyOption\"\r\n                                :key=\"item.value\"\r\n                                :label=\"item.label\"\r\n                                :value=\"item.value\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所在镇\" >\r\n                        <el-select v-model=\"form.town\" placeholder=\"请选择所在镇\" :disabled='form.county==\"\"?true:false'>\r\n                            <el-option\r\n                                v-for=\"item in townOption\"\r\n                                :key=\"item.id\"\r\n                                :label=\"item.town\"\r\n                                :value=\"item.id\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所在村\">\r\n                        <el-select v-model=\"form.village\" placeholder=\"请选择所在村\" :disabled='form.county==\"\"||form.town==\"\"?true:false'>\r\n                            <el-option\r\n                                v-for=\"item in villageOption\"\r\n                                :key=\"item.id\"\r\n                                :label=\"item.village\"\r\n                                :value=\"item.id\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" @click=\"search\">查询</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'side',\r\n    data(){\r\n        return{\r\n            form:{\r\n                county:'',\r\n                town:'',\r\n                village:'',\r\n            },\r\n            //1=>定海区，2=>普陀区,3=>岱山县,4=>嵊泗县\r\n            countyOption:[\r\n                {\r\n                    value: 1,\r\n                    label: '定海区'\r\n                }, \r\n                {\r\n                    value: 2,\r\n                    label: '普陀区'\r\n                }, \r\n                {\r\n                    value: 3,\r\n                    label: '岱山县'\r\n                }, \r\n                {\r\n                    value: 4,\r\n                    label: '嵊泗县'\r\n                }, \r\n            ],\r\n            townOption:[],\r\n            villageOption:[],\r\n            json_fields:{\r\n                所在区:\"countyCotent\",\r\n                所在镇:\"town\",\r\n                所在村:\"village\",\r\n                海水养殖面积:\"seaArea\",\r\n                海水养殖估算面积:\"seaEstimateArea\",\r\n                淡水养殖面积:\"freshWaterArea\",\r\n                淡水养殖估算面积:\"freshWaterEstimateArea\",\r\n                养殖总面积:\"totalArea\",\r\n                养殖估算总面积:\"totalEstimateArea\",\r\n            },\r\n            json_data:[\r\n\r\n            ]\r\n        }\r\n    },\r\n    methods:{\r\n        //获取镇和村\r\n        getTown(){\r\n            let data=this.qs.stringify({\r\n                countyId:this.form.county,\r\n            })\r\n\r\n            this.$axios({\r\n                url:'/api/area/getTown',\r\n                method:'POST',\r\n                data:data\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==200){\r\n                    this.townOption=res.data.data.data\r\n                    //赋值\r\n                }\r\n            })\r\n        },\r\n        getVillage(){\r\n\r\n            let data=this.qs.stringify({\r\n                Tid:this.form.town,\r\n            })\r\n\r\n            this.$axios({\r\n                url:'/api/area/getVillage',\r\n                method:'POST',\r\n                data:data\r\n            }).then(res=>{\r\n                if(res.data.status==200){\r\n                    this.villageOption=res.data.data.data\r\n                }\r\n            })\r\n        },\r\n        search(){\r\n            let data=this.qs.stringify(this.form)\r\n            this.$axios({\r\n                url:'/api/ponds/search',\r\n                method:'POST',\r\n                data:data\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==200){\r\n                    this.$store.commit('statistic/setPondsType',res.data.data.data)\r\n                }\r\n            })\r\n        },\r\n        exportExcel(){\r\n            this.json_data=this.PondsType\r\n        }\r\n    },\r\n    computed:{\r\n        PondsType(){\r\n            return this.$store.state.statistic.pondsType\r\n        }\r\n    },\r\n    watch:{\r\n        'form.county'(n,o){\r\n            if(n=='')return\r\n            //重新选择\r\n            if(n!==o){\r\n                //清空\r\n                this.form.town=''\r\n                this.form.village=''\r\n            }\r\n            //重新获取town数据\r\n            this.getTown()\r\n        },\r\n        'form.town'(n,o){\r\n            if(n=='')return\r\n            //重新选择\r\n            if(n!==o){\r\n                //清空\r\n                this.form.village=''\r\n            }\r\n            this.getVillage()\r\n        },\r\n        'form.village'(n,o){\r\n            if(n=='')return\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}