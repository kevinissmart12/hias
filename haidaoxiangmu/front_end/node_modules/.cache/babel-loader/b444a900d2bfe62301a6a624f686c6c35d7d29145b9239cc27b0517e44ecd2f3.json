{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'allProducts',\n  data() {\n    return {\n      tempTableData: [],\n      tableItem: [{\n        'id': '1',\n        prop: 'name',\n        label: '名称',\n        width: '180'\n      }, {\n        'id': '2',\n        prop: 'imgUrl',\n        label: '图片',\n        width: '180'\n      }, {\n        'id': '3',\n        prop: 'type',\n        label: '所属种类',\n        width: '180'\n      }, {\n        'id': '4',\n        prop: 'breedMethod',\n        label: '养殖方式',\n        width: '180'\n      },\n      // {'id':'5',prop:'breedTech',label:'养殖技术',width:'300'}, \n      // {'id':'6',prop:'description',label:'描述',width:'300'}, \n      // {'id':'7',prop:'diseases',label:'病害',width:'300'},\n      {\n        'id': '8',\n        prop: 'checkStatus',\n        label: '审核状况',\n        width: '100'\n      }],\n      currentPage: 1,\n      pagesize: 5,\n      dialogVisible: false,\n      productInfo: {}\n    };\n  },\n  methods: {\n    getProducts() {\n      this.$axios({\n        url: '/api/products/getAll',\n        method: 'GET'\n      }).then(res => {\n        if (res.data.status == 200) {\n          this.$message.success(res.data.data.msg);\n          this.tempTableData = res.data.data.data.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n          this.$store.commit('allProducts/setTableData', res.data.data.data);\n        }\n      });\n    },\n    checkProduct(item) {\n      this.dialogVisible = true;\n      this.productInfo = item;\n    },\n    goProductEdit(item) {\n      this.$router.push({\n        path: '/products/add',\n        query: {\n          id: item.id\n        }\n      });\n    },\n    deleteProduct(item) {\n      let data = this.qs.stringify(item);\n      this.$axios({\n        url: '/api/products/delete',\n        method: 'POST',\n        data: data\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 200) {\n          this.$message.success(res.data.data.msg);\n        }\n      });\n    },\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.tempTableData = this.TableData.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n    },\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      this.tempTableData = this.TableData.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n    }\n  },\n  created() {\n    this.getProducts();\n  },\n  computed: {\n    TableLength() {\n      return this.$store.state.allProducts.tableData.length;\n    },\n    TableData() {\n      return this.$store.state.allProducts.tableData;\n    }\n  }\n};","map":{"version":3,"mappings":";AAsFA;EACAA;EACAC;IACA;MAEAC;MACAC,YACA;QAAA;QAAAC;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,GACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA;MACA;MACA;MACA;MACA;QAAA;QAAAF;QAAAC;QAAAC;MAAA,EAEA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;QACAC;QACAC;MACA;QAEA;UACA;UACA;UACA;QACA;MAEA;IACA;IACAC;MACA;MACA;IAEA;IACAC;MACA;QACAC;QACAC;UACAC;QACA;MACA;IAEA;IACAC;MACA;MAEA;QACAP;QACAC;QACAb;MACA;QACAoB;QACA;UACA;QACA;MACA;IAGA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;IAEA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","tempTableData","tableItem","prop","label","width","currentPage","pagesize","dialogVisible","productInfo","methods","getProducts","url","method","checkProduct","goProductEdit","path","query","id","deleteProduct","console","handleSizeChange","handleCurrentChange","created","computed","TableLength","TableData"],"sourceRoot":"src/components/products","sources":["AllProducts.vue"],"sourcesContent":["<template>\r\n    <div class=\"allProducts\">\r\n        <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <span>养殖信息列表</span>\r\n            </div>\r\n\r\n            <el-table\r\n                :data=\"tempTableData\"\r\n                style=\"width: 100%\">\r\n                <el-table-column\r\n                    :prop=\"item.prop\"\r\n                    :label=\"item.label\"\r\n                    :width=\"item.width\"\r\n                    v-for=\"(item,index) in tableItem\" \r\n                    :key=\"index\"\r\n                >\r\n                    <template slot-scope=\"scope\">\r\n                        <img v-if=\"item.prop=='imgUrl'\" :src=\"scope.row.imgUrl\" alt=\"\" width=\"90\">\r\n                        <div v-else>{{scope.row[item.prop]}}</div>\r\n                    </template>\r\n                </el-table-column>\r\n\r\n                <el-table-column\r\n                    fixed=\"right\"\r\n                    label=\"操作\"\r\n                    width=\"150\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button @click=\"checkProduct(scope.row)\" type=\"text\" size=\"small\">查看</el-button>\r\n                        <el-button @click=\"goProductEdit(scope.row)\" type=\"text\" size=\"small\">编辑</el-button>\r\n                        <el-button @click=\"deleteProduct(scope.row)\" style=\"color:red\" type=\"text\" size=\"small\" >删除</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n\r\n            </el-table>\r\n\r\n            <!--  分页 -->\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"currentPage\"\r\n                :page-sizes=\"[5,10]\"\r\n                :page-size=\"pagesize\"\r\n                layout=\"total, sizes, prev, pager, next\"\r\n                :total=\"TableLength\">\r\n            </el-pagination>\r\n\r\n        </el-card>\r\n\r\n        <el-dialog\r\n            title=\"养殖信息\"\r\n            :visible.sync=\"dialogVisible\"\r\n            width=\"50%\"\r\n        >\r\n            <table class=\"dialog_table\" >\r\n                <tr>\r\n                    <td class=\"col_head\">品种名称</td>\r\n                    <td>{{productInfo.name}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"col_head\">所属种类</td>\r\n                    <td>{{productInfo.type}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td class=\"col_head\">所属种类</td>\r\n                    <td>{{productInfo.breedMethod}}</td>\r\n                </tr>\r\n                <!-- <tr>\r\n                    <td>描述</td>\r\n                    <td>{{productInfo.description}}</td>\r\n                </tr> -->\r\n            </table>\r\n\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogVisible = false\">关闭</el-button>\r\n            </span>\r\n        </el-dialog>\r\n\r\n\r\n\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'allProducts',\r\n    data() {\r\n        return {\r\n\r\n            tempTableData:[],\r\n            tableItem:[\r\n                {'id':'1',prop:'name',label:'名称',width:'180'},\r\n                {'id':'2',prop:'imgUrl',label:'图片',width:'180'},\r\n                {'id':'3',prop:'type',label:'所属种类',width:'180'},\r\n                {'id':'4',prop:'breedMethod',label:'养殖方式',width:'180'}, \r\n                // {'id':'5',prop:'breedTech',label:'养殖技术',width:'300'}, \r\n                // {'id':'6',prop:'description',label:'描述',width:'300'}, \r\n                // {'id':'7',prop:'diseases',label:'病害',width:'300'},\r\n                {'id':'8',prop:'checkStatus',label:'审核状况',width:'100'},\r\n\r\n            ],\r\n            currentPage:1,\r\n            pagesize:5,\r\n            dialogVisible: false,\r\n            productInfo:{},\r\n        }\r\n    },\r\n    methods:{\r\n        getProducts(){\r\n            this.$axios({\r\n                url:'/api/products/getAll',\r\n                method:'GET'\r\n            }).then(res=>{\r\n\r\n                if(res.data.status==200){\r\n                    this.$message.success(res.data.data.msg);\r\n                    this.tempTableData=res.data.data.data.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n                    this.$store.commit('allProducts/setTableData',res.data.data.data)\r\n                }\r\n\r\n            })\r\n        },\r\n        checkProduct(item){\r\n            this.dialogVisible=true\r\n            this.productInfo=item\r\n            \r\n        },\r\n        goProductEdit(item){\r\n            this.$router.push({\r\n                path:'/products/add',\r\n                query:{\r\n                    id:item.id\r\n                }\r\n            })\r\n\r\n        },\r\n        deleteProduct(item){\r\n            let data =this.qs.stringify(item)\r\n\r\n            this.$axios({\r\n                url:'/api/products/delete',\r\n                method:'POST',\r\n                data:data\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==200){\r\n                    this.$message.success(res.data.data.msg);\r\n                }\r\n            })\r\n\r\n\r\n        },\r\n        handleSizeChange (size) {\r\n            this.pagesize = size;\r\n            this.tempTableData=this.TableData.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n        },\r\n        handleCurrentChange (currentPage) {\r\n            this.currentPage = currentPage;\r\n            this.tempTableData=this.TableData.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n\r\n        },\r\n    },\r\n    created(){\r\n        this.getProducts()\r\n    },\r\n    computed:{\r\n        TableLength(){\r\n            return this.$store.state.allProducts.tableData.length\r\n        },\r\n        TableData(){\r\n            return this.$store.state.allProducts.tableData\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n    .allProducts{\r\n        position: relative;\r\n        width: 100%;\r\n        height: 100%;\r\n        .el-card{\r\n            margin-left:12.5%;\r\n            width: 75%;\r\n            height: 100%;\r\n        }\r\n    }\r\n    .dialog_table{\r\n        width: 100%;\r\n        border: 1px solid #000 ;\r\n        border-spacing: 20px 10px;\r\n        tr{\r\n            width: 100%;\r\n            border-bottom: 1px solid #000;\r\n            .col_head{\r\n                width: 30%;\r\n            }\r\n            \r\n        }\r\n    }\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}