{"ast":null,"code":"export default {\n  name: 'mainContent',\n  data() {\n    return {\n      discussionList: [],\n      currentPage: 1,\n      pagesize: 5\n    };\n  },\n  methods: {\n    getDiscussion() {\n      this.$axios({\n        url: '/api/discussion/getAll',\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 401) return;\n        if (res.data.status !== 200) {\n          return this.$message.error(res.data.data.msg);\n        } else {\n          this.discussionList = res.data.data.data.slice((this.currentPage - 1) * this.pagesize, this.currentPage * this.pagesize);\n          //store\n          this.$store.commit('discussion/setDiscussionList', res.data.data.data);\n        }\n      });\n    }\n  },\n  created() {\n    this.getDiscussion();\n  },\n  computed: {\n    Discussion() {\n      return this.$store.state.discussion.discussionList;\n    },\n    DiscussionLength() {\n      return this.$store.state.discussion.discussionList.length;\n    }\n  },\n  watch: {\n    Discussion(n, o) {\n      console.log(n, o);\n    }\n  }\n};","map":{"version":3,"mappings":"AAeA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MAEA;QACAC;QACAC;MACA;QACAC;QACA;QACA;UACA;QACA;UACA;UACA;UACA;QACA;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAF;MACAH;IACA;EACA;AACA","names":["name","data","discussionList","currentPage","pagesize","methods","getDiscussion","url","method","console","created","computed","Discussion","DiscussionLength","watch"],"sourceRoot":"src/components/discussion","sources":["Main.vue"],"sourcesContent":["<template>\r\n    <div class=\"mainContent\">\r\n        <el-card class=\"card\">\r\n            <div slot=\"header\" class=\"clearfix\">\r\n                <span>卡片名称</span>\r\n                <el-button style=\"float: right; padding: 3px 0\" type=\"text\">操作按钮</el-button>\r\n            </div>\r\n            <div v-for=\"o in 4\" :key=\"o\" class=\"text item\">\r\n                {{'列表内容 ' + o }}\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'mainContent',\r\n    data(){\r\n        return{\r\n            discussionList:[],\r\n            currentPage:1,\r\n            pagesize:5,\r\n        }\r\n    },\r\n    methods:{\r\n        getDiscussion(){\r\n\r\n            this.$axios({\r\n                url:'/api/discussion/getAll',\r\n                method:\"GET\"\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==401)return\r\n                if(res.data.status!==200){\r\n                    return this.$message.error(res.data.data.msg);\r\n                }else{\r\n                    this.discussionList=res.data.data.data.slice((this.currentPage-1)*this.pagesize,this.currentPage*this.pagesize)\r\n                    //store\r\n                    this.$store.commit('discussion/setDiscussionList',res.data.data.data)\r\n                }\r\n            })\r\n        }\r\n    },\r\n    created(){\r\n        this.getDiscussion()\r\n    },\r\n    computed:{\r\n        Discussion(){\r\n            return this.$store.state.discussion.discussionList\r\n        },\r\n        DiscussionLength(){\r\n            return this.$store.state.discussion.discussionList.length\r\n        }\r\n    },\r\n    watch:{\r\n        Discussion(n,o){\r\n            console.log(n,o);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .mainContent{\r\n        width: 100%;\r\n        height: 100%;\r\n\r\n        .card{\r\n            width: 100%;\r\n            height: 100%;\r\n            overflow-y: hidden;\r\n        }\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}