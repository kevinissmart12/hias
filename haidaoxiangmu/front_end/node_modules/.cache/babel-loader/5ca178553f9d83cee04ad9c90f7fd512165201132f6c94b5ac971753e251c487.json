{"ast":null,"code":"export default {\n  name: \"AddControl\",\n  data() {\n    return {\n      drawing: 0,\n      text: '添加池塘',\n      type: 'primary',\n      form: {\n        name: '',\n        owner: ''\n      }\n    };\n  },\n  methods: {\n    //进入绘图编辑模式\n    drawMode() {\n      if (this.drawing == 0) {\n        this.drawing = 1;\n      } else if (this.drawing == 1) {\n        this.drawing = 0;\n      }\n      if (this.drawing == 2) {\n        this.drawing = 3;\n      } else if (this.drawing == 3) {\n        this.drawing = 2;\n      }\n      this.$store.commit('ponds/setDrawing', this.drawing);\n    },\n    //获取owner\n    getOwner() {\n      this.$axios({\n        url: '/api/owner/getAll',\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 200) {}\n      });\n    }\n  },\n  created() {\n    this.getOwner();\n  },\n  computed: {\n    Drawing() {\n      return this.$store.state.ponds.drawing;\n    }\n  },\n  watch: {\n    Drawing(n) {\n      this.drawing = n;\n      if (n == 0) {\n        this.text = '添加池塘';\n        this.type = \"primary\";\n      } else if (n == 1) {\n        this.text = '停止绘制';\n        this.type = \"danger\";\n      } else if (n == 2) {\n        this.text = '编辑池塘';\n        this.type = \"success\";\n      } else if (n == 3) {\n        this.text = '停止编辑';\n        this.type = \"danger\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAsBA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAL;QACAM;MACA;IACA;EACA;EACAC;IACA;IACAC;MACA;QACA;MACA;QACA;MACA;MAEA;QACA;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACA;QACAC;QACAC;MACA;QACAC;QACA,6BAEA;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IAEAC;MACA;IACA;EAEA;EACAC;IACAD;MACA;MACA;QACA;QACA;MACA;QACA;QACA;MACA;QACA;QACA;MACA;QACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","drawing","text","type","form","owner","methods","drawMode","getOwner","url","method","console","created","computed","Drawing","watch"],"sourceRoot":"src/components/ponds/