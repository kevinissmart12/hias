{"ast":null,"code":"export default {\n  name: \"side\",\n  data() {\n    return {\n      form: {\n        uid: '',\n        username: '',\n        type: '',\n        op_obj: '',\n        front: 0,\n        end: 10\n      }\n    };\n  },\n  methods: {\n    search() {\n      let data = this.qs.stringify(this.form);\n      this.$axios({\n        url: \"/api/dialog/search\",\n        method: \"POST\",\n        data: data\n      }).then(res => {\n        console.log(res.data);\n        if (res.data.status == 200) {\n          this.$store.commit('dialog/setTableData', res.data.data.data);\n          this.$store.commit('dialog/setCurrentPage', 1);\n          this.$store.commit('dialog/setSearch', true);\n        }\n      });\n    },\n    selectUser() {\n      this.UserData.forEach((i, v) => {\n        if (i.id == this.form.uid) {\n          this.form.username = i.name;\n          this.form.uid = i.id;\n        }\n      });\n    },\n    vanishUser() {\n      this.form.username = '';\n      this.form.uid = '';\n    },\n    quit() {\n      this.form = {\n        uid: '',\n        username: '',\n        type: '',\n        op_obj: '',\n        front: 0,\n        end: 10\n      };\n      this.$store.commit('dialog/setCurrentPage', 1);\n      this.$store.commit('dialog/setSearch', true);\n    }\n  },\n  computed: {\n    UserData() {\n      return this.$store.state.dialog.userData;\n    },\n    CurrentPage() {\n      return this.$store.state.dialog.currentPage;\n    },\n    Pagesize() {\n      return this.$store.state.dialog.pageSize;\n    }\n  }\n};","map":{"version":3,"mappings":"AA2CA;EACAA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;MAEA;QACAC;QACAC;QACAX;MACA;QACAY;QACA;UACA;UACA;UACA;QAEA;MACA;IACA;IACAC;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;QACAb;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACA;MACA;IACA;EAEA;EACAS;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AAEA","names":["name","data","form","uid","user