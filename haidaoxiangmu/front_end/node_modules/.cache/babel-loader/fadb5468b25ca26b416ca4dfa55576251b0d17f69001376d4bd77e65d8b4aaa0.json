           <el-button type=\"primary\" @click=\"search\">查询</el-button>\r\n                    <el-button type=\"danger\" @click=\"quit\">去除条件</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:\"side\",\r\n    data(){\r\n        return{\r\n            form: {\r\n                uid: '',\r\n                username:'',\r\n                type:'',\r\n                op_obj:'',\r\n                front:0,\r\n                end:10,\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        search(){\r\n            let data=this.qs.stringify(this.form)\r\n\r\n            this.$axios({\r\n                url:\"/api/dialog/search\",\r\n                method:\"POST\",\r\n                data:data\r\n            }).then(res=>{\r\n                console.log(res.data);\r\n                if(res.data.status==200){\r\n                    this.$store.commit('dialog/setTableData',res.data.data.data)\r\n                    this.$store.commit('dialog/setCurrentPage',1)\r\n                    this.$store.commit('dialog/setSearch',true)\r\n\r\n                }\r\n            })\r\n        },\r\n        selectUser(){\r\n            this.UserData.forEach((i,v)=>{\r\n                if(i.id==this.form.uid){\r\n                    this.form.username=i.name\r\n                    this.form.uid=i.id\r\n                }\r\n            })\r\n        },\r\n        vanishUser(){\r\n            this.form.username=''\r\n            this.form.uid=''\r\n        },\r\n        quit(){\r\n            this.form={\r\n                uid: '',\r\n                username:'',\r\n                type:'',\r\n                op_obj:'',\r\n                front:0,\r\n                end:10,\r\n            }\r\n                                this.$store.commit('dialog/setCurrentPage',1)\r\n                    this.$store.commit('dialog/setSearch',true)\r\n        }\r\n\r\n    },\r\n    computed:{\r\n        UserData(){\r\n            return this.$store.state.dialog.userData\r\n        },\r\n        CurrentPage(){\r\n            return this.$store.state.dialog.currentPage\r\n        },        \r\n        Pagesize(){\r\n            return this.$store.state.dialog.pageSize\r\n        },\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    .side{\r\n        width: 100%;\r\n        height: 100%;\r\n\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}